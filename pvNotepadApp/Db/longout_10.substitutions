#Substitution file for 10 records
file longout.template
{
    pattern { RECORD           }
            { $(HUTCH):NOTE:LONG:01 }
            { $(HUTCH):NOTE:LONG:02 }
            { $(HUTCH):NOTE:LONG:03 }
            { $(HUTCH):NOTE:LONG:04 }
            { $(HUTCH):NOTE:LONG:05 }
            { $(HUTCH):NOTE:LONG:06 }
            { $(HUTCH):NOTE:LONG:07 }
            { $(HUTCH):NOTE:LONG:08 }
            { $(HUTCH):NOTE:LONG:09 }
            { $(HUTCH):NOTE:LONG:10 }
}
